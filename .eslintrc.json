{
	"extends": "airbnb/legacy",
	"plugins": ["angular"],
	"env": {
		"browser": true,
		"node": true
	},
	"globals": {
		"angular": true,
		"module": true,
		"inject": true,
		"cordova": true,
		"StatusBar": true
	},
	"rules": {
		"new-cap": 1,
		"no-console": 0,
		"func-names": 1,
		"max-len": [2, 120, 2],
		"space-before-function-paren": 0,
    "no-shadow": 1
	}
}
